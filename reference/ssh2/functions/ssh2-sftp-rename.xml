<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.ssh2-sftp-rename">
 <refnamediv>
  <refname>ssh2_sftp_rename</refname>
  <refpurpose>Renomeia um arquivo remoto</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>ssh2_sftp_rename</methodname>
   <methodparam><type>resource</type><parameter>sftp</parameter></methodparam>
   <methodparam><type>string</type><parameter>from</parameter></methodparam>
   <methodparam><type>string</type><parameter>to</parameter></methodparam>
  </methodsynopsis>
  <para>
   Renomeia um arquivo no sistema de arquivos remoto.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>sftp</parameter></term>
     <listitem>
      <para>
       Um recurso SSH2 SFTP aberto por <function>ssh2_sftp</function>.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>from</parameter></term>
     <listitem>
      <para>
       O arquivo atual que est√° sendo renomeado.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>to</parameter></term>
     <listitem>
      <para>
       O novo nome de arquivo que substitui <parameter>from</parameter>.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Renomeando um arquivo via SFTP</title>
    <programlisting role="php">
<![CDATA[
<?php
$connection = ssh2_connect('shell.example.com', 22);
ssh2_auth_password($connection, 'usuario', 'senha');
$sftp = ssh2_sftp($connection);

ssh2_sftp_rename($sftp, '/home/usuario/nomeantigo', '/home/usuario/nomenovo');
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>rename</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

