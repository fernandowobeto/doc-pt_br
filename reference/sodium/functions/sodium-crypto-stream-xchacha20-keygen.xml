<refentry xml:id="function.sodium-crypto-stream-xchacha20-keygen" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>sodium_crypto_stream_xchacha20_keygen</refname>
  <refpurpose>Retorna uma chave segura aleatória</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>sodium_crypto_stream_xchacha20_keygen</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Retorna uma chave segura aleatória para uso com <function>sodium_crypto_stream_xchacha20</function>.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna uma chave 32-byte segura aleatória para uso com <function>sodium_crypto_stream_xchacha20</function>.
  </para>
 </refsect1>

</refentry>
