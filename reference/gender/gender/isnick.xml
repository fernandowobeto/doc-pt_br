<refentry xml:id="gender-gender.isnick" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Gender\Gender::isNick</refname>
  <refpurpose>Verifica se um nome é apelido de outro</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>array</type><methodname>Gender\Gender::isNick</methodname>
   <methodparam><type>string</type><parameter>name0</parameter></methodparam>
   <methodparam><type>string</type><parameter>name1</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>country</parameter></methodparam>
  </methodsynopsis>
  <para>
   Verifica se name0 é apelido de name1.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>name0</parameter></term>
    <listitem>
     <para>
      Nome a verificar.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>name1</parameter></term>
    <listitem>
     <para>
      Nome a verificar.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>country</parameter></term>
    <listitem>
     <para>
      ID do país identificado pela constante de classe Gender. Se omitido, ANY_COUNTRY é usado.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>


</refentry>

