<refentry xml:id="gender-gender.get" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Gender\Gender::get</refname>
  <refpurpose>Obtém o gênero de um nome</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>Gender\Gender::get</methodname>
   <methodparam><type>string</type><parameter>name</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>country</parameter></methodparam>
  </methodsynopsis>
  <para>
   Obtém o gênero do nome em um país específico.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>name</parameter></term>
    <listitem>
     <para>
      Nome a verificar.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>country</parameter></term>
    <listitem>
     <para>
      ID do país identificado pela constante de classe Gender.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna o gênero do nome.
  </para>
 </refsect1>


</refentry>

