<refentry xml:id="componere.cast" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Componere\cast</refname>
  <refpurpose>Conversão</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>object</type><methodname>Componere\cast</methodname>
   <methodparam><type>string</type><parameter>type</parameter></methodparam>
   <methodparam><type>object</type><parameter>object</parameter></methodparam>
  </methodsynopsis>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>type</parameter></term>
    <listitem>
     <para>
      Um tipo definido pelo usuário.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>object</parameter></term>
    <listitem>
     <para>
      Um objeto com um tipo definido pelo usuário, compatível com <exceptionname>Type</exceptionname>
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Um <type>object</type> do tipo <exceptionname>Type</exceptionname>, convertido de <parameter>object</parameter>
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <warning>
   <para>
    Lança uma <type>InvalidArgumentException</type> se o tipo de <parameter>object</parameter> for uma classe interna ou derivado de uma classe interna.
   </para>
  </warning>
  <warning>
   <para>
    Lança uma <type>InvalidArgumentException</type> se <exceptionname>Type</exceptionname> for uma interface
   </para>
  </warning>
  <warning>
   <para>
    Lança uma <type>InvalidArgumentException</type> se <exceptionname>Type</exceptionname> for um trait
   </para>
  </warning>
  <warning>
   <para>
    Lança uma <type>InvalidArgumentException</type> se <exceptionname>Type</exceptionname> for um abstract
   </para>
  </warning>
  <warning>
   <para>
    Lança uma <type>InvalidArgumentException</type> se <exceptionname>Type</exceptionname> não for compatível com o tipo de <parameter>object</parameter>
   </para>
  </warning>
 </refsect1>

 <refsect1 role="seealso">
   &reftitle.seealso;
   <simplelist>
    <member><xref linkend="componere.cast_by_ref" /></member>
   </simplelist>
  </refsect1>

</refentry>
