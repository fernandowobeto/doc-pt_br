<refentry xml:id="function.runkit7-constant-remove" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>runkit7_constant_remove</refname>
  <refpurpose>
   Remove/exclui uma constante jรก definida
  </refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>runkit7_constant_remove</methodname>
   <methodparam><type>string</type><parameter>constant_name</parameter></methodparam>
  </methodsynopsis>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>constant_name</parameter></term>
    <listitem>
     <para>
      Nome da constante a ser removida. Pode ser o nome de uma constante global,
      ou <literal>classname::constname</literal>, indicando uma constante de classe.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>define</function></member>
    <member><function>runkit7_constant_add</function></member>
    <member><function>runkit7_constant_redefine</function></member>
   </simplelist>
  </para>
 </refsect1>


</refentry>

