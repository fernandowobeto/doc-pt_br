<refentry xml:id="iteratoraggregate.getiterator" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>IteratorAggregate::getIterator</refname>
  <refpurpose>Recupera um iterador ou Traversable externo</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="IteratorAggregate">
   <modifier>public</modifier> <type>Traversable</type><methodname>IteratorAggregate::getIterator</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Retorna um iterador ou Traversable externo.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Uma instância de um objeto que implementa <classname>Iterator</classname> ou
   <classname>Traversable</classname>
  </para>
 </refsect1>

 <refsect1 role="errors"><!-- {{{ -->
  &reftitle.errors;
  <para>
   Lança uma <classname>Exception</classname> em caso de falha.
  </para>
 </refsect1><!-- }}} -->

</refentry>
